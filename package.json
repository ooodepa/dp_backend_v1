{
  "license": "GPL-3.0",
  "scripts": {
    "cpanel-migr-run": "npx typeorm --dataSource dist/migration.config.js migration:run",
    "cpanel-migr-rev": "npx typeorm --dataSource dist/migration.config.js migration:revert",
    "dev-start": "cross-env NODE_ENV=dev nest start --watch",
    "dev-debug-start": "cross-env NODE_ENV=dev nest start --debug --watch",
    "dev-typeorm": "cross-env NODE_ENV=dev typeorm --dataSource dist/migration.config.js",
    "dev-migr-gen": "yarn build && yarn dev-typeorm migration:generate --pretty true",
    "dev-migr-run": "yarn build && yarn dev-typeorm migration:run",
    "dev-migr-rev": "yarn dev-typeorm migration:revert",
    "dev-db": "yarn dev-db-down && yarn dev-db-up",
    "dev-db-up": "docker-compose -f docker-compose.db-dev.yml --env-file ./docker-compose.db-dev.env up",
    "dev-db-down": "docker-compose -f docker-compose.db-dev.yml --env-file ./docker-compose.db-dev.env down",
    "prod-start": "cross-env NODE_ENV=prod node dist/main",
    "prod-debug-start": "cross-env NODE_ENV=prod nest start --debug --watch",
    "prod-typeorm": "cross-env NODE_ENV=prod typeorm --dataSource dist/migration.config.js",
    "prod-migr-gen": "yarn build && yarn prod-typeorm migration:generate --pretty true",
    "prod-migr-run": "yarn build && yarn prod-typeorm migration:run",
    "prod-migr-rev": "yarn prod-typeorm migration:revert",
    "prod-db": "yarn prod-db-down && yarn prod-db-up",
    "prod-db-up": "docker-compose -f docker-compose.db-prod.yml --env-file ./docker-compose.db-prod.env up",
    "prod-db-down": "docker-compose -f docker-compose.db-prod.yml --env-file ./docker-compose.db-prod.env down",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.{ts,json,md}\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "exceljs": "^4.3.0",
    "js2xmlparser": "^5.0.0",
    "mysql2": "^3.2.4",
    "nodemailer": "^6.9.1",
    "number-to-words-ru": "^2.3.6",
    "preview-email": "^3.0.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cryptojs": "^3.1.30",
    "@types/ejs": "^3.0.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/pug": "2.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "ejs": "^3.1.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "4.0.0",
    "handlebars": "^4.7.6",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "pug": "^3.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4",
    "webpack": "^5.0.0"
  }
}
